{
    "family": "nithin-strapi-final-v13-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::811738710312:role/ecs_fargate_taskRole",
    "containerDefinitions": [
        {
            "name": "strapi",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 1337,
                    "hostPort": 1337,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                { "name": "NODE_ENV", "value": "production" },
                { "name": "DATABASE_HOST", "value": "nithin-strapi-final-v13-db.cibg8qw2kouc.us-east-1.rds.amazonaws.com" },
                { "name": "DATABASE_PORT", "value": "5432" },
                { "name": "DATABASE_NAME", "value": "strapi" },
                { "name": "DATABASE_USERNAME", "value": "postgres" },
                { "name": "DATABASE_PASSWORD", "value": "YourSecurePassword123!" },
                { "name": "DATABASE_CLIENT", "value": "postgres" },
                { "name": "DATABASE_SSL", "value": "true" },
                { "name": "NODE_TLS_REJECT_UNAUTHORIZED", "value": "0" },
                { "name": "APP_KEYS", "value": "mySecretKey1,mySecretKey2" },
                { "name": "API_TOKEN_SALT", "value": "myApiTokenSalt" },
                { "name": "ADMIN_JWT_SECRET", "value": "myAdminJwtSecret" },
                { "name": "TRANSFER_TOKEN_SALT", "value": "myTransferTokenSalt" },
                { "name": "JWT_SECRET", "value": "myJwtSecret" }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/nithin-strapi-final-v13",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024"
}